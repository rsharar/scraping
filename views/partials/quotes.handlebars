{{#each quotes}}
<li class="quotes list-group-item">
    <h5>{{quote}}</h5>
    <p>{{author}}</p>
    <p><i>More from<a href={{link}}>{{author}}</a></i></p>
    <a href={{img}}></a>
    <div class="row">
        <div class="col text-right">
            <button class="btn collapsed btn-info" data-target="#a{{_id}}"><small>Comments</small></button>
        </div>
    </div>
</li>
{{/each}}